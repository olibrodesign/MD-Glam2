		jQuery(document).ready(function(){
			jQuery('.next_button').click(function(){
				// alert('aaaaaaaaa');
				var currentNextStep = parseInt(jQuery('.work_steps.active').attr('data-step'));
				var currentNextStepPlus = parseInt(currentNextStep)+1;
				// alert(currentNextStepPlus);
				jQuery('.how_it_works_step'+currentNextStep).hide();
				jQuery('.how_it_works_step'+currentNextStep).removeClass('active');
				jQuery('.how_it_works_step'+currentNextStepPlus).show();
				jQuery('.how_it_works_step'+currentNextStepPlus).addClass('active');
				if(currentNextStepPlus == '4'){
					jQuery('.next_button').hide();
				}else{
					jQuery('.next_button').show();
				}
				if(currentNextStepPlus >= '2'){
					jQuery('.pre_button').show();
				}else{
					jQuery('.pre_button').hide();
				}
			});
			jQuery('.pre_button').click(function(){
				var currentPreStep =  parseInt(jQuery('.work_steps.active').attr('data-step'));
				var currentPreStepMinus = parseInt(currentPreStep)-1;
				// alert(currentPreStepMinus);
				jQuery('.how_it_works_step'+currentPreStep).hide();
				jQuery('.how_it_works_step'+currentPreStep).removeClass('active');
				jQuery('.how_it_works_step'+currentPreStepMinus).show();
				jQuery('.how_it_works_step'+currentPreStepMinus).addClass('active');
				if(currentPreStepMinus == '1'){
					jQuery('.pre_button').hide();
				}else{
					jQuery('.pre_button').show();
				}
				if(currentPreStepMinus <= '3'){
					jQuery('.next_button').show();
				}else{
					jQuery('.next_button').hide();
				}
			});
		});
		</script>
